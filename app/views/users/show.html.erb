<% provide(:title, @user.name) %>
<div class = "row">
  <aside class = "col-md-4">
    <section class = "user_info">
      <h1>
       <%= render 'shared/user_info' %>
      </h1>
    </section>
  </aside>

  <div class = "col-md-8">

  	<% if @user.workouts.any? %>
  		<h3> Activity Feed</h3>
  		<ol class = "workouts">
  	   <%= render 'shared/feed' %>
  	</ol>
  	<%end%>
  </div>
</div>
